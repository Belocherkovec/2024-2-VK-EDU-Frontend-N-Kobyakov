{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@features/*": ["features/*"],
      "@consts/*": ["consts/*"],
      "@public/*": ["public/*"],
      "@icons/*": ["icons/*"],
      "@img/*": ["img/*"],
      "@app/*": ["app/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "include": ["src/**/*"]
}
